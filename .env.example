# Bot Configuration
BOT_TOKEN=your_bot_token_here

# Admin Configuration (comma-separated telegram user IDs)
ADMIN_IDS=123456789,987654321

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/rsb_tgbot?schema=public

# Userbot Configuration (optional, for auto-forwarding feature)
USERBOT_API_ID=your_api_id
USERBOT_API_HASH=your_api_hash
USERBOT_SESSION=your_session_string

# Bot Username (without @)
BOT_USERNAME=your_bot_username

# Proxy Configuration (required in China mainland)
# HTTP_PROXY=http://127.0.0.1:7890
# HTTPS_PROXY=http://127.0.0.1:7890

# Environment
NODE_ENV=development
